WITH START AS(
SELECT START_DATE,
    ROW_NUMBER() OVER (ORDER BY START_DATE) AS ROW_NO
    FROM PROJECTS
    WHERE START_DATE NOT IN (SELECT END_DATE
                            FROM PROJECTS)
),
END AS(
 SELECT END_DATE,
    ROW_NUMBER() OVER (ORDER BY END_DATE) AS ROW_NO
    FROM PROJECTS
    WHERE END_DATE NOT IN(SELECT START_DATE
                         FROM PROJECTS)
)
SELECT S.START_DATE,E.END_DATE
FROM START S
JOIN END E ON S.ROW_NO=E.ROW_NO
ORDER BY DATEDIFF(END_DATE,START_DATE),S.START_DATE
